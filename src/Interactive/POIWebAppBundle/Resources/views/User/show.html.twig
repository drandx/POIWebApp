{% extends 'POIWebAppBundle:Administrator:dashboard.html.twig' %}
{% set menu_selected = 'three' %}
{% block content %}
<h2 class="sub-header">Usuario</h2>
<dl>
    <dt>Id</dt>
    <dd>{{ entity.id }}</dd>
    <dt>Nombre</dt>
    <dd>{{ entity.firstName }}</dd>
    <dt>Apellido</dt>
    <dd>{{ entity.lastName }}</dd>
    <dt>Email</dt>
    <dd>{{ entity.email }}</dd>
</dl>

<ul class="record_actions">
    <li>
        <a href="{{ path('poi_user') }}">
            Volver a la lista
        </a>
    </li>
    <li>
        <a href="{{ path('poi_user_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
